    <script>
        // Animated counter for metrics
        function animateCounter(element) {
            const target = parseInt(element.getAttribute('data-target'));
            const duration = 2000;
            const increment = target / (duration / 16);
            let current = 0;

            const timer = setInterval(() => {
                current += increment;
                if (current >= target) {
                    element.textContent = target.toLocaleString() + '+';
                    clearInterval(timer);
                } else {
                    element.textContent = Math.floor(current).toLocaleString();
                }
            }, 16);
        }

        // Intersection Observer for metrics
        const metricsObserver = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    const numbers = entry.target.querySelectorAll('.metric-number');
                    numbers.forEach(num => {
                        if (num.textContent === '0') {
                            animateCounter(num);
                        }
                    });
                }
            });
        }, { threshold: 0.5 });

        const metricsSection = document.querySelector('.metrics-grid');
        if (metricsSection) {
            metricsObserver.observe(metricsSection.parentElement);
        }

        // Smooth scroll for internal links
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        // Form submission
        const form = document.getElementById('partnershipForm');
        if (form) {
            form.addEventListener('submit', function(e) {
                e.preventDefault();
                
                // Get form data
                const formData = new FormData(form);
                const data = Object.fromEntries(formData);
                
                // Show success message
                alert('Thank you for your partnership request! Our team will review your submission and get back to you within 48 hours.');
                
                // Reset form
                form.reset();
                
                // Scroll to top
                window.scrollTo({ top: 0, behavior: 'smooth' });
            });
        }

        // Scroll animations for cards
        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const scrollObserver = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);

        // Apply scroll animations to various elements
        document.querySelectorAll('.benefit-card, .model-card, .package-card, .partner-logo, .flow-step').forEach(el => {
            el.style.opacity = '0';
            el.style.transform = 'translateY(30px)';
            el.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
            scrollObserver.observe(el);
        });

        // Add staggered animation delays
        document.querySelectorAll('.benefits-grid .benefit-card').forEach((card, index) => {
            card.style.transitionDelay = `${index * 0.1}s`;
        });

        document.querySelectorAll('.models-grid .model-card').forEach((card, index) => {
            card.style.transitionDelay = `${index * 0.1}s`;
        });

        document.querySelectorAll('.packages-container .package-card').forEach((card, index) => {
            card.style.transitionDelay = `${index * 0.15}s`;
        });

        document.querySelectorAll('.partners-grid .partner-logo').forEach((logo, index) => {
            logo.style.transitionDelay = `${index * 0.05}s`;
        });

        // Pulsing animation for connection nodes
        const nodes = document.querySelectorAll('.node');
        nodes.forEach((node, index) => {
            node.style.animationDelay = `${index * 0.3}s`;
        });
    </script>
